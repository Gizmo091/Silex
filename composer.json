{
    "name": "silexandfriends/silex",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "https://silex.symfony.com",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        },
        {
            "name": "Roberto Nygaard",
            "email": "roberto@nygaard.es"
        }
    ],
    "require": {
        "php": ">=7.4|^8.0",
        "pimple/pimple": "^3.0",
        "symfony/event-dispatcher": "^5",
        "symfony/http-foundation": "^5",
        "symfony/http-kernel": "^5",
        "symfony/monolog-bridge": "^5",
        "symfony/routing": "^4.4",
        "symfony/security-bundle": "^5"
    },
    "require-dev": {
        "doctrine/dbal": "^2.2",
        "fig/link-util": "^1.1",
        "monolog/monolog": "^1.4.1",
        "rector/rector-prefixed": "^0.9.31",
        "swiftmailer/swiftmailer": "^5",
        "symfony/asset": "^4.4",
        "symfony/browser-kit": "^4.4",
        "symfony/config": "^5",
        "symfony/css-selector": "^4.4",
        "symfony/debug": "^4.4",
        "symfony/doctrine-bridge": "^5",
        "symfony/dom-crawler": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/form": "^5",
        "symfony/intl": "^4.4",
        "symfony/options-resolver": "^5",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/process": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/translation": "^5",
        "symfony/twig-bridge": "^5",
        "symfony/validator": "^5",
        "symfony/var-dumper": "^4.4",
        "symfony/web-link": "^4.4",
        "twig/twig": "^3"
    },
    "replace": {
        "silex/api": "self.version",
        "silex/providers": "self.version",
        "silex/silex": "self.version"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5.x-dev"
        }
    }
}
