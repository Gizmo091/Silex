{
    "name": "silexandfriends/silex",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "https://silex.symfony.com",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        },
        {
            "name": "Roberto Nygaard",
            "email": "roberto@nygaard.es"
        }
    ],
    "require": {
        "php": "^8.0",
        "pimple/pimple": "^3.4",
        "symfony/event-dispatcher": "~5.2.0",
        "symfony/http-foundation": "~5.2.0",
        "symfony/http-kernel": "~5.2.0",
        "symfony/monolog-bridge": "~5.2.0",
        "symfony/routing": "~5.2.0",
        "symfony/security-bundle": "~5.2.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.2",
        "fig/link-util": "^1.1",
        "monolog/monolog": "^2.2",
        "rector/rector-prefixed": "^0.9.31",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/asset": "~5.2.0",
        "symfony/browser-kit": "~5.2.0",
        "symfony/config": "~5.2.0",
        "symfony/css-selector": "~5.2.0",
        "symfony/doctrine-bridge": "~5.2.0",
        "symfony/dom-crawler": "~5.2.0",
        "symfony/expression-language": "~5.2.0",
        "symfony/finder": "~5.2.0",
        "symfony/form": "~5.2.0",
        "symfony/intl": "~5.2.0",
        "symfony/options-resolver": "~5.2.0",
        "symfony/phpunit-bridge": "~5.2.0",
        "symfony/process": "~5.2.0",
        "symfony/serializer": "~5.2.0",
        "symfony/translation": "~5.2.0",
        "symfony/twig-bridge": "~5.2.0",
        "symfony/validator": "~5.2.0",
        "symfony/var-dumper": "~5.2.0",
        "symfony/web-link": "~5.2.0",
        "twig/twig": "^3"
    },
    "replace": {
        "silex/api": "self.version",
        "silex/providers": "self.version",
        "silex/silex": "self.version"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
